                <ul class="sidebar-menu" data-widget="tree">
                    <li class="text-center header">MENU</li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-dashboard"></i> <span>Dashboards</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="../../index.html"><i class="fa fa-circle-o"></i> Geral Tempo Real</a></li>
                            <li><a href="../../index2.html"><i class="fa fa-circle-o"></i> Dashboard v2</a></li>
                        </ul>
                    </li>

                    <li> <a href="https://adminlte.io/docs"><i class="glyphicon glyphicon-shopping-cart"></i> <span>Lojas</span></a></li>
                    <li> <a href="https://adminlte.io/docs"><i class="glyphicon glyphicon-user"></i> <span>Meu Perfil</span></a></li>
                    <li> <a href="https://adminlte.io/docs"><i class="glyphicon glyphicon-flash"></i> <span>Meu Plano</span></a></li>
                       
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">
            @RenderBody()
        </div>

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
            </div>
            <strong>Copyright &copy; <a href="https://adminlte.io">BestStore</a>.</strong> All rights
            reserved.
        </footer>

         <div class="control-sidebar-bg"></div>
